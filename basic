name=input("Enter your name : ")
print(name)
NoE =input("years of experience : ")
print("you have {} of experience".format(NoE))
x= int(NoE)
if bool(x) == True:
    if x <= 3:
        print("entry level")
    elif x >3 and x<=6:
       print("intermediate")
    else:
     print("Exp")
else:
    print("Fresher")




